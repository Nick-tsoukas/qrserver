{
  "kind": "collectionType",
  "collectionName": "support_moments",
  "info": {
    "singularName": "support-moment",
    "pluralName": "support-moments",
    "displayName": "Support Moment",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "band": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::band.band",
      "inversedBy": "support_moments"
    },
    "bandNameSnapshot": {
      "type": "string"
    },
    "bandSlugSnapshot": {
      "type": "string"
    },
    "bandImageSnapshot": {
      "type": "string"
    },
    "buttonKey": {
      "type": "string"
    },
    "supportLabel": {
      "type": "string"
    },
    "badgeId": {
      "type": "string"
    },
    "amount": {
      "type": "integer"
    },
    "currency": {
      "type": "string"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "pending",
        "paid",
        "failed",
        "refunded"
      ],
      "default": "pending"
    },
    "paidAt": {
      "type": "datetime"
    },
    "stripePaymentIntentId": {
      "type": "string"
    },
    "stripeChargeId": {
      "type": "string"
    },
    "fanDisplayName": {
      "type": "string"
    },
    "fanHandle": {
      "type": "string"
    },
    "fanMessage": {
      "type": "string"
    },
    "fanEmail": {
      "type": "string"
    },
    "shareOptIn": {
      "type": "boolean",
      "default": false
    },
    "showAmountOnShare": {
      "type": "boolean",
      "default": false
    },
    "supporterWallOptIn": {
      "type": "boolean",
      "default": false
    },
    "source": {
      "type": "string",
      "default": "band-profile"
    },
    "qrId": {
      "type": "string"
    },
    "eventId": {
      "type": "string"
    }
  }
}
